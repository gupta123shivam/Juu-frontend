{
  "expo": {
    "scheme": "newapp",
    "web": {
      "bundler": "metro"
    },
    "name": "new-app",
    "slug": "new-app"
  },
  "plugins": [
    "expo-camera",
    {
      "cameraPermission": "Allow $(MyApp) to access your camera."
    }
  ],
  "splash": {
    "image": "./src/assets/splash.png",
    "backgroundColor": "#FEF9B0",
    "resizeMode": "cover"
  },
  "android": {
    "package": "com.shivamgupta4891651.newapp",
    "adaptiveIcon": {
      "foregroundImage": "./src/assets/adaptive-icon.png",
      "backgroundColor": "#FEF9B0"
    },
    "permissions": ["CAMERA"],
    "userInterfaceStyle": "automatic"
  },
  "extra": {
    "eas": {}
  }
}
