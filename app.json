{
  "expo": {
    "scheme": "newapp",
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "4f7f61d5-e107-4c17-b27f-7e388cbd2acf"
      }
    },
    "name": "newapp",
    "slug": "newapp",
    "sdkVersion": "48.0.0",
    "entryPoint": ".vscode\\exponentIndex.js",
    "jsEngine": "hermes"
  },
  "plugins": [
    "expo-camera",
    {
      "cameraPermission": "Allow $(MyApp) to access your camera."
    }
  ],
  "splash": {
    "image": "./src/assets/splash.png",
    "backgroundColor": "#FEF9B0",
    "resizeMode": "cover"
  },
  "android": {
    "package": "com.shivamgupta4891651.newapp",
    "adaptiveIcon": {
      "foregroundImage": "./src/assets/adaptive-icon.png",
      "backgroundColor": "#FEF9B0"
    },
    "permissions": [
      "CAMERA"
    ],
    "userInterfaceStyle": "automatic"
  },
  "name": "newapp"
}