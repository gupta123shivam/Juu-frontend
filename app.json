{
  "expo": {
    "scheme": "newapp",
    "web": {
      "bundler": "metro"
    },
    "name": "new-app",
    "slug": "new-app"
  },
  "plugins": [
    "expo-camera",
    {
      "cameraPermission": "Allow $(MyApp) to access your camera."
    }
  ],
  "splash": {
    "image": "./assets/splash.png",
    "backgroundColor": "#FEF9B0",
    "resizeMode": "cover"
  },
  "android": {
    "adaptiveIcon": {
      "foregroundImage": "./assets/adaptive-icon.png",
      "backgroundColor": "#FEF9B0"
    },
    "permissions": ["CAMERA"]
  }
}
